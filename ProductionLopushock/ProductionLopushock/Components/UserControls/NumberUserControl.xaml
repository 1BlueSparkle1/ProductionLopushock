<UserControl x:Class="ProductionLopushock.Components.UserControls.NumberUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProductionLopushock.Components.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Button x:Name="NumberBtn" BorderBrush="White" Foreground="Black" Background="White" Click="NumberBtn_Click">
            <TextBlock x:Name="NumberTb"></TextBlock>
        </Button>
    </Grid>
</UserControl>
